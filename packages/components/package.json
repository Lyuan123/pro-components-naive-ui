{
  "name": "pro-naive-ui",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.0.6",
  "description": "基于 naive-ui 二次封装，让中后台开发更简单",
  "author": "zhengchangfu",
  "license": "MIT",
  "homepage": "https://naive-ui.pro-components.cn/zh-CN/os-theme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zheng-Changfu/pro-naive-ui"
  },
  "keywords": [
    "naive-ui",
    "pro-naive-ui",
    "component library",
    "ui framework",
    "ui",
    "vue"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "unpkg": "./dist/index.js",
  "jsdelivr": "./dist/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib"
  ],
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "test:watch": "vitest",
    "test": "vitest run",
    "clean": "rimraf es lib dist",
    "build2": "rollup -c --bundleConfigAsCjs",
    "build": "pnpm clean && rollup -c --bundleConfigAsCjs"
  },
  "peerDependencies": {
    "naive-ui": "^2.38.2",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@vue/test-utils": "^2.4.6",
    "deepmerge": "^4.3.1",
    "jsdom": "^24.0.0",
    "rollup": "^4.28.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "vitest": "^1.6.0"
  }
}
